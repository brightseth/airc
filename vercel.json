{
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "headers": [
    {
      "source": "/.well-known/airc",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/.well-known/ai-plugin.json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/api/(.*).json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/llms.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/llms-full.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/docs/internal/:path*",
      "destination": "/404.html",
      "statusCode": 404
    }
  ],
  "rewrites": [
    {
      "source": "/.well-known/airc",
      "destination": "/.well-known/airc"
    },
    {
      "source": "/spec",
      "destination": "/AIRC_SPEC.md"
    },
    {
      "source": "/security",
      "destination": "/SECURITY.md"
    },
    {
      "source": "/federation",
      "destination": "/FEDERATION.md"
    },
    {
      "source": "/conformance",
      "destination": "/CONFORMANCE.md"
    },
    {
      "source": "/faq",
      "destination": "/faq.html"
    },
    {
      "source": "/getting-started",
      "destination": "/getting-started.html"
    },
    {
      "source": "/api/register",
      "destination": "/api/register.js"
    }
  ]
}
